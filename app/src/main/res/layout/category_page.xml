                android:fontFamily="@font/montserrat_semibold"
                android:text="@string/_24795tg"
                android:textColor="@color/black"
                android:textSize="7sp"
                app:layout_constraintTop_toTopOf="@id/FFtext42" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view_divider_second"
        android:layout_width="0dp"
        android:layout_height="5dp"
        app:layout_constraintEnd_toEndOf="@id/TopSellers2"
        app:layout_constraintStart_toStartOf="@id/TopSellers2"
        app:layout_constraintTop_toBottomOf="@id/TopSellers2" />

    <LinearLayout
        android:id="@+id/TopSellers3"
        android:layout_width="320dp"
        android:layout_height="165dp"
        android:layout_marginTop="6dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/view_divider_second">

        <LinearLayout
            android:id="@+id/TopLine5"
            android:layout_width="162dp"
            android:layout_height="171dp"
