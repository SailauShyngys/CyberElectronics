<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bluegray"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/categ"
        android:layout_width="325dp"
        android:layout_height="55dp"
        android:layout_marginTop="54dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/arrow_back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/arrow_background2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/arrow"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:layout_marginStart="-10dp"
                android:background="@null"
                android:contentDescription="@string/back_button"
                android:src="@drawable/arrow_back1" />
        </LinearLayout>

        <TextView
            android:id="@+id/Htext"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_marginStart="55dp"
            android:layout_marginTop="-1dp"
            android:fontFamily="@font/montserrat_semibold"
            android:letterSpacing="0.05"
            android:text="@string/power_units1"
            android:textColor="@color/purple"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/TopSellers1"
        android:layout_width="320dp"
        android:layout_height="190dp"
        android:layout_marginTop="25dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/categ">

        <LinearLayout
            android:id="@+id/TopLine1"
            android:layout_width="162dp"
            android:layout_height="171dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/prod_background"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/ibtnFFirst1"
                android:layout_width="110dp"
                android:layout_height="112dp"
                android:layout_marginHorizontal="25dp"
                android:layout_marginTop="9dp"
                android:contentDescription="@string/image_button"
                android:src="@drawable/prod1" />

            <TextView
                android:id="@+id/FFtext11"
                android:layout_width="110dp"
                android:layout_height="10dp"
                android:layout_marginHorizontal="23.5dp"
                android:fontFamily="@font/montserrat_semibold"
                android:text="@string/hyper_x_cloud_stinger"
                android:textColor="@color/purple"
                android:textSize="8sp"
                app:layout_constraintTop_toTopOf="@id/ibtnFFirst1" />

            <TextView
                android:id="@+id/FFtext12"
                android:layout_width="95dp"
                android:layout_height="8dp"
                android:layout_marginHorizontal="23.5dp"
                android:fontFamily="@font/montserrat_light"
                android:text="@string/headphones_and_headsets"
                android:textColor="@color/black"
                android:textSize="6sp"
                app:layout_constraintTop_toTopOf="@id/FFtext11" />

            <TextView
                android:id="@+id/FFtext13"
                android:layout_width="95dp"
                android:layout_height="9.5dp"
                android:layout_marginHorizontal="23.5dp"
                android:fontFamily="@font/montserrat_semibold"
                android:text="@string/_24795tg"
                android:textColor="@color/black"
                android:textSize="7sp"
                app:layout_constraintTop_toTopOf="@id/FFtext12" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/TopLine2"
            android:layout_width="162dp"
            android:layout_height="171dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/prod_background"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/TopLine"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/ibtnFFirst2"
                android:layout_width="110dp"
                android:layout_height="112dp"
                android:layout_marginHorizontal="25dp"
                android:layout_marginTop="9dp"
